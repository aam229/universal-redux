    "ignore-loader": "^0.1.2",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "lodash": "^4.11.1",
    "node-sass": "4.5.3",
    "pretty-error": "2.1.0",
    "prettyjson": "1.2.1",
