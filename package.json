    "redux-devtools": "3.4.0",
    "redux-devtools-dock-monitor": "1.1.2",
    "redux-devtools-log-monitor": "1.3.0",
    "redux-logger": "3.0.6",
    "sass-loader": "6.0.5",
    "serialize-javascript": "1.3.0",
    "serve-favicon": "2.4.3",
    "style-loader": "0.17.0",
