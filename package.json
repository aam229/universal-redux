  },
  "bin": {
    "universal-redux-server-dev-build": "./bin/server/server-dev-build.js",
    "universal-redux-server-build": "./bin/server/server-build.js",
    "universal-redux-server-dll-build": "./bin/dll/server-dll-build.js",
    "universal-redux-server-dll-analysis": "./bin/dll/server-dll-analyze.js",
    "universal-redux-client-dev-server": "./bin/client/client-dev-server.js",
    "universal-redux-client-build": "./bin/client/client-build.js",
    "universal-redux-client-dll-build": "./bin/dll/client-dll-build.js",
    "universal-redux-client-dll-analysis": "./bin/dll/client-dll-analyze.js"
